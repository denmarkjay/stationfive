{
    "name": "stationfive",
    "version": "1.0.0",
    "description": "hambleton assets compiler",
    "repository": "https://github.com/denmarkjay/stationfive",
    "author": "Denmark Jay Mago",
    "main": "script.js",
    "license": "MIT",
    "private": true,
    "scripts": {
        "webpack": "webpack --no-progress --hide-modules --compress --colors --config=webpack.config",
        "build:production": "NODE_ENV=production SITE_URL='http://localhost:8000' npm run webpack",
        "build:staging": "NODE_ENV=production SITE_URL='http://localhost:8000' npm run webpack",
        "build:dev": "cross-env NODE_ENV=development SITE_URL='http://localhost:8000' npm run webpack",
        "watch": "cross-env NODE_ENV=development SITE_URL='http://localhost:8000' webpack --watch --progress --hide-modules --config=webpack.config"
    },
    "browserslist": [
        "last 2 versions",
        "not ie <= 9",
        "not ie_mob <= 9"
    ],
    "dependencies": {
        "@babel/cli": "^7.2.0",
        "@babel/core": "^7.2.0",
        "@babel/polyfill": "^7.0.0",
        "@babel/preset-env": "^7.2.0",
        "babel-core": "^6.26.3",
        "babel-loader": "^8.0.2",
        "babel-plugin-module-resolver": "^3.1.1",
        "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
        "bootstrap": "^4.6.0",
        "css-loader": "^1.0.1",
        "dotenv-webpack": "^1.5.7",
        "eslint-plugin-jquery": "^1.3.2",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "file-loader": "^2.0.0",
        "html-webpack-plugin": "^3.2.0",
        "image-webpack-loader": "^4.6.0",
        "jquery": "^1.12.4",
        "jquery.mousewheel": "^3.1.9",
        "lodash": "^4.17.11",
        "mini-css-extract-plugin": "^0.4.5",
        "node-sass": "^4.10.0",
        "popper.js": "^1.15.0",
        "prettier": "^1.15.3",
        "remove-files-webpack-plugin": "^1.1.3",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "webpack": "^4.27.1",
        "webpack-dev-middleware": "^3.4.0",
        "webpack-merge": "^4.1.4"
    },
    "devDependencies": {
        "babel-plugin-transform-class-properties": "^6.24.1",
        "browser-sync": "^2.26.5",
        "browser-sync-webpack-plugin": "^2.2.2",
        "cross-env": "^5.2.0",
        "eslint": "^5.9.0",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-plugin-import": "^2.14.0",
        "event-hooks-webpack-plugin": "^2.1.1",
        "expose-loader": "^0.7.5",
        "optimize-css-assets-webpack-plugin": "^5.0.1",
        "terser-webpack-plugin": "^1.2.3",
        "url-loader": "^1.1.2",
        "webpack-cli": "^3.3.2",
        "webpack-dev-server": "^3.1.10"
    }
}
